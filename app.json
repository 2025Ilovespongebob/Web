{
  "expo": {
    "name": "sea-vision-rn",
    "slug": "sea-vision-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "seavisionrn",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "경로 찾기를 위해 현재 위치가 필요합니다.",
        "NSCameraUsageDescription": "실시간 영상 스트리밍을 위해 카메라가 필요합니다.",
        "NSMicrophoneUsageDescription": "영상 녹화를 위해 마이크가 필요합니다.",
        "LSApplicationQueriesSchemes": [
          "kakaolink",
          "kakaokompassauth",
          "kakaoplus",
          "kakaolink",
          "kakaokompassauth",
          "kakaoplus"
        ]
      },
      "bundleIdentifier": "com.dehgns12.seavisionrn"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "kakaolink"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "kakaolink"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.dehgns12.seavisionrn"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@react-native-kakao/core",
        {
          "nativeAppKey": "99e3fd064582ce7387fe6b1bc3eb1e9a",
          "android": {
            "customScheme": "kakao99e3fd064582ce7387fe6b1bc3eb1e9a"
          },
          "ios": {
            "customScheme": "kakao99e3fd064582ce7387fe6b1bc3eb1e9a"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
